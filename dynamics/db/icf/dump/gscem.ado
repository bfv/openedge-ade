<dataset_transaction TransactionNo="131" TransactionType="DATA"><contained_record DB="icfdb" Table="gsc_entity_mnemonic" deletion_flag="no" entity_mnemonic="gscem" import_version_number_seq="4.09" key_field_value="3000049853.09" record_version_obj="3000049854.09" secondary_key_value="GSTCS" version_date="08/05/2003" version_number_seq="4.09" version_time="24200" version_user="admin"><entity_mnemonic>GSTCS</entity_mnemonic>
<entity_mnemonic_short_desc>context scope</entity_mnemonic_short_desc>
<entity_mnemonic_description>gst_context_scope</entity_mnemonic_description>
<auto_properform_strings>yes</auto_properform_strings>
<entity_mnemonic_label_prefix></entity_mnemonic_label_prefix>
