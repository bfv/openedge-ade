<iframe name="win9" border=0 frameborder="no" class="hide" application="yes" onload="if(window.fLoad) fLoad()"></iframe>
</td></tr></table>


<div id="info" mode="auto" style="height:100px;width:99%;"></div>


<form name="form" method="post" target="run" action="" class="hide">
  <input type="hidden" name="lookup" value="">
  <input type="hidden" name="wscharset" value="UTF-8">
  <input type="hidden" name="sessionid" value="">
  <input type="hidden" name="lognote" value="">
</form>

<script language="Javascript" src="../dhtml/ryinithtml.js"></script>
<script language="Javascript">

// When not using extension mapping then set the "dyn" property of the <td id="app" dyn=""> tag.
// ie <td id="app" dyn="/scripts/cgiip.exe/WService=wsdynamics1/dhtml/">

// Layout specific code
function run(){
//  window.mainmenu=window.treeview; // Menu tools from MenuController     (ie, treeview)
//  window.mainband=window.treeview; // Menu entries from MenuController     (ie, treeview)
  window.mainmenu=window.menubar; // Menu tools from MenuController     (ie, treeview)
  window.mainband=window.menubar; // Menu entries from MenuController     (ie, treeview)
  window.progmenu=window.menubar;  // Local menubar entries from programs 
  window.progband=window.menubar;  // Local program entries to menubar
}
</script>
